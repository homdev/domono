{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "rules": {
    "@next/next/no-html-link-for-pages": "off",
    "react/react-in-jsx-scope": "off",
    "react/no-unescaped-entities": "off",
    "react/no-children-prop": "off",
    "react/no-unknown-property": "off",
    "no-unused-vars": "off"
  },
  "globals": {
    "React": "writable"
  }
}
